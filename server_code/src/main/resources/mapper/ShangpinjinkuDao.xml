<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cl.dao.ShangpinjinkuDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cl.entity.ShangpinjinkuEntity" id="shangpinjinkuMap">
        <result property="shangpinmingcheng" column="shangpinmingcheng"/>
        <result property="alllimittimes" column="alllimittimes"/>
        <result property="shangpintupian" column="shangpintupian"/>
        <result property="jinkushijian" column="jinkushijian"/>
    </resultMap>

    <select id="selectListView"
		resultType="com.cl.entity.view.ShangpinjinkuView" >

		SELECT  shangpinjinku.* FROM shangpinjinku  shangpinjinku 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.cl.entity.view.ShangpinjinkuView" >
		SELECT * FROM shangpinjinku  shangpinjinku <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
